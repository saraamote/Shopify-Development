/**********

collection page

***********/

/* product form */

.temporary-product-form {

  border-bottom: 1px solid #000;
  border-top: 1px solid #000;
  margin: 3% 6%;
  font-size: 0;
  padding: 0;

}

/* inquire, price, purchse */

.product-description-purchase,
.product-description-inquire,
.product-description-price {

  font: 100 15px/1.5em "Proxima Nova Condensed", "Avenir Next Condensed", sans-serif;
  text-transform: uppercase;
  display: inline-block;
  text-align: center;
  padding: 2% 0;
  width: 33.33%;

}

/* desc */

.product-description-details {

  margin: 3% 6%;

}

/* designer bio */

.designer-bio {

  font: 300 15px/22px 'Proxima Nova Condensed', 'Avenir Next Condensed', sans-serif;
  font-variant: normal;
  font-style: normal;
  text-align: center;
  margin: 3% 6%;

}

/* product grid */

.collection-grid {

  position: relative;
  background: #000;

}

.temporary-collection-grid img {

  width: 33.33%;

}

